{
  "name": "@unocss-applet/preset-applet",
  "type": "module",
  "version": "0.7.8",
  "description": "The Applet preset for [UnoCSS](https://github.com/unocss/unocss).",
  "author": "Neil Lee <zguolee@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/unocss-applet/unocss-applet/tree/main/packages/preset-applet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unocss-applet/unocss-applet.git",
    "directory": "packages/preset-applet"
  },
  "bugs": "https://github.com/unocss-applet/unocss-applet/issues",
  "keywords": [
    "unocss",
    "unocss-preset",
    "uni-app"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "devDependencies": {
    "@unocss/core": "^0.56.5",
    "@unocss/preset-mini": "^0.56.5",
    "@unocss/preset-uno": "^0.56.5",
    "unocss": "^0.56.5"
  },
  "peerDependencies": {
    "@unocss/core": ">=0.56",
    "@unocss/preset-mini": ">=0.56",
    "@unocss/preset-uno": ">=0.56",
    "unocss": ">=0.56"
  },
  "peerDependenciesMeta": {
    "@unocss/core": {
      "optional": true
    },
    "@unocss/preset-mini": {
      "optional": true
    },
    "@unocss/preset-uno": {
      "optional": true
    }
  }
}
